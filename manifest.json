{
    "content_scripts": [{
        "all_frames": true,
        "css": ["css/scrollbar.css"],
        "js": ["contentscript/gesture.js"],
        "matches": ["\u003Call_urls\u003E"],
        "run_at": "document_start"
    }, {
        "all_frames": true,
        "js": ["lib/jquery-1.8.2.min.js",
               "contentscript/contentscript.js"],
        "matches":  [ "http://*/*", "https://*/*" ],
        "run_at": "document_start"
    }, {
        "css": ["css/youdaoreader.css"],
        "matches": ["http://reader.youdao.com/*"],
        "run_at": "document_start"
    }],

    "background": {
        "persistent": true,
        "scripts": ["background/gesture.js",
                    "background/background.js"]
    },

    "chrome_url_overrides": {
        "history": "background/betterhistory/index.html"
    },

    "browser_action": {
        "default_icon": "css/img/icon64.png",
        "default_title": "多合一扩展",
        "default_popup": "popup/popup.html"
    },

    "permissions": [
        "tabs",
        "downloads",
        "history",
        "management",
        "\u003Call_urls\u003E"
    ],

    "web_accessible_resources": [
        "contenscript/*",
        "background/*"
     ],

    "icons": {
        "128": "css/img/icon128.png",
        "64": "css/img/icon64.png",
        "32": "css/img/icon32.png"
    },

    "manifest_version": 2,

    "name": "多合一扩展",

    "description": "多合一扩展",

    "version": "2014.6.10",

    "default_locale": "en"
}
