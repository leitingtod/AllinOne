{
    "background": {
        "persistent": true,
        "scripts": ["gesture/background.js", "float-video/background.js"]
    },

    "content_scripts": [{
        "all_frames": true,
        "css": ["css/scrollbar.css", "popup/css/material-icons.css"],
        "js": ["gesture/contentscript.js", "float-video/contentscript.js", "float-video/jquery-3.3.1.min.js"],
        "matches": ["\u003Call_urls\u003E"],
        "run_at": "document_start"
    }],

    "chrome_url_overrides": {
        "history": "betterhistory/index.html"
    },

    "browser_action": {
        "default_icon": "css/img/icon64.png",
        "default_title": "多合一扩展",
        "default_popup": "popup/popup.html"
    },

    "permissions": [
        "tabs",
        "downloads",
        "sessions",
        "history",
        "management",
        "\u003Call_urls\u003E"
    ],

    "web_accessible_resources": [
        "contenscript/*",
        "background/*",
        "css/*"
    ],

    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

    "icons": {
        "128": "css/img/icon128.png",
        "64": "css/img/icon64.png",
        "32": "css/img/icon32.png"
    },

    "default_locale": "en",

    "manifest_version": 2,

    "name": "多合一扩展Test",

    "description": "多合一扩展",

    "version": "2014.6.10"
}
